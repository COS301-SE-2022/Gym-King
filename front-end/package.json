{
  "name": "front-end",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@capacitor/android": "4.2.0",
    "@capacitor/app": "1.1.1",
    "@capacitor/cli": "^4.0.0",
    "@capacitor/core": "^4.0.0",
    "@capacitor/device": "^4.0.1",
    "@capacitor/geolocation": "^4.0.1",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/ios": "4.2.0",
    "@capacitor/keyboard": "1.2.2",
    "@capacitor/status-bar": "1.0.8",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/express": "^4.17.13",
    "@types/node": "*",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "ionicons": "^5.4.0",
    "jest": "28.0.0",
    "react-scripts": "^5.0.1",
    "swiper": "^8.2.6",
    "ts-node": "^10.7.0",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)"
    ]
  },
  "scripts": {
    "serve:prod": "env-cmd -f .env.production react-scripts start",
    "serve:dev": "env-cmd -f .env.development react-scripts start",
    "build": "yarn nx build:ios && yarn nx build:android",
    "build:ios": " ionic cap build ios env-cmd -f .env.production && echo y | npx --silent trapeze run trapeze.yml",
    "build:android": " ionic cap build android env-cmd -f .env.production && echo y | npx --silent trapeze run trapeze.yml",
    "test": "env-cmd -f .env.development react-scripts  test --transformIgnorePatterns --watchAll=false",
    "eject": "react-scripts eject",
    "lint:fix": "eslint src/**/*.ts --fix",
    "lint": "eslint src/**/*.ts",
    "prettify": "prettier src/**/*.ts --write",
    "affected": "react-scripts affected"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "eslint-plugin-prettier": "^4.0.0"
  },
  "description": "An Ionic project"
}
